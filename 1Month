  // ‡§Ö‡§∏‡§≤‡•Ä ‡§™‡§æ‡§∏‡§µ‡§∞‡•ç‡§°

// ‡§™‡§æ‡§∏‡§µ‡§∞‡•ç‡§° ‡§è‡§Ç‡§ü‡§∞ ‡§ï‡§∞‡§®‡•á ‡§ï‡•á ‡§≤‡§ø‡§è ‡§´‡§Ç‡§ï‡•ç‡§∂‡§®
function getPassword() {
    echo "[+] Enter Key: ";
    return trim(fgets(STDIN));
}

// ‡§™‡§æ‡§∏‡§µ‡§∞‡•ç‡§° ‡§ú‡§æ‡§Ç‡§ö‡§®‡•á ‡§ï‡§æ ‡§´‡§Ç‡§ï‡•ç‡§∂‡§®
function checkPassword($pw) {
    $attempt = getPassword();
    if ($attempt === $pw) {
        echo "\033[1;32m‚úÖ Password correct!\n\033[0m";
        sleep(2);
        file_put_contents("key", password_hash($pw, PASSWORD_BCRYPT));
        return true;
    }
    echo "\033[1;31m‚ùå Wrong password!\n\033[0m";
    return false;
}

// ‡§™‡§π‡§≤‡•Ä ‡§¨‡§æ‡§∞ ‡§™‡§æ‡§∏‡§µ‡§∞‡•ç‡§° ‡§ö‡•á‡§ï ‡§ï‡§∞‡•á‡§Ç
if (!checkPassword($pw)) {
    unlink("key");

    // ‡§¨‡§æ‡§∞-‡§¨‡§æ‡§∞ ‡§™‡§æ‡§∏‡§µ‡§∞‡•ç‡§° ‡§Æ‡§æ‡§Ç‡§ó‡•á‡§Ç ‡§ú‡§¨ ‡§§‡§ï ‡§∏‡§π‡•Ä ‡§® ‡§π‡•ã
    while (!checkPassword($pw)) {
        sleep(2);
    }
}

echo "‚úÖ Access Granted!\n";








